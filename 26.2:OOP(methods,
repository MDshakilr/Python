#function/method

class Car:

    def welcome(self):
     print("welcome men")

    def __init__(x,name,mark):
        x.nam=name  #object atribute
        x.mark=mark



s1 = Car("bmw",45)
s1.welcome()
print(s1.nam)
print(s1.mark)
s2 = Car("toyota",47)
print(s2.mark)
s3 = Car("mercedes",0)
print(s3.nam,s3.mark)
