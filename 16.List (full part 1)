#squre bracket =[]
#changeable/muteable
#list type
list1=["apple","mango","banna"]
list2=[1,2,3,4,8]
print(type(list1))
print(list2)

list1[1]="guava"
print(list1)

#append
list2=[1,2,3,4,8]
list2.append(10)
print(list2)

#insert
list1=["apple","mango","banna"]
list1.insert(2,"orange")
print(list1)

list2=[1,2,3,4,8]
list2.insert(1,5)
print(list2)

#remove
list2=[1,2,3,4,8]
list2.remove(3)
print(list2)

list = ["apple", "banana", "cherry"]
list.remove("banana")
print(list)

#Remove Specified Index
list = ["apple", "banana", "cherry"]
list.pop(1)
print(list)

list = ["apple", "banana", "cherry"]
list.pop()
print(list)

list = ["apple", "banana", "cherry"]
del list[0] #The del keyword also removes the specified index:
print(list)

#The clear() method empties the list.

list = ["apple", "banana", "cherry"]
list.clear()
print(list)
